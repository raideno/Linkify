{
  "name": "linkify",
  "description": "Linkify will help you to save & manage links that you need",
  "version": "1.0",
  "author": "raiden",
  "manifest_version": 2,
  "icons": {
    "16": "/images/icon-16.png",
    "48": "/images/icon-48.png",
    "128": "/images/icon-128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "/images/icon-16.png",
      "48": "/images/icon-48.png",
      "128": "/images/icon-128.png"
    },
    "default_popup": "index.html"
  },
  "permissions": ["storage", "tabs"]
}
